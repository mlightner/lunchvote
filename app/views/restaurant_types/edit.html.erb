<h1>Editing restaurant_type</h1>

<% form_for(@restaurant_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @restaurant_type %> |
<%= link_to 'Back', restaurant_types_path %>
